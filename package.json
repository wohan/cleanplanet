    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "firebase": "^7.18.0",
    "mobx": "^5.15.6",
    "mobx-react": "^6.2.5",
    "moment": "^2.27.0",
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-image-picker": "^2.3.3",
    "react-native-maps": "0.27.1",
